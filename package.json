{
  "name": "capacitor-stripe-terminal",
  "version": "0.0.1",
  "description": "Capacitor plugin for Stripe Terminal (credit card readers).",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "prepare": "husky install",
    "pre-commit": "pretty-quick --staged",
    "docs": "typedoc --excludeExternals --out out src/index.ts"
  },
  "author": "eventOne Labs <opensource@event1.io>",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "^5.6.0",
    "@stripe/terminal-js": "^0.12.0",
    "lodash": "^4.17.21",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@capacitor/android": "^5.6.0",
    "@capacitor/ios": "^5.6.0",
    "@types/lodash": "^4.14.202",
    "husky": "^8.0.3",
    "prettier": "^3.2.4",
    "prettier-plugin-java": "^2.5.0",
    "pretty-quick": "^4.0.0",
    "semantic-release": "^23.0.0",
    "typedoc": "^0.25.7",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorStripeTerminal.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "stripe",
    "stripe-terminal"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eventOneHQ/capacitor-stripe-terminal"
  },
  "bugs": {
    "url": "https://github.com/eventOneHQ/capacitor-stripe-terminal/issues"
  },
  "release": {
    "branches": [
      {
        "name": "next"
      },
      {
        "name": "beta",
        "channel": "beta",
        "prerelease": true
      }
    ]
  }
}
